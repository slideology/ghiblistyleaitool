# 本地开发环境变量
# 复制此文件为 .env 并填入真实值

# Cloudflare配置
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token_here
CLOUDFLARE_ACCOUNT_ID=your_account_id_here

# 应用配置
SESSION_SECRET=your_session_secret_here
CDN_URL=your_r2_cdn_url_here
DOMAIN=http://localhost:5173/

# Google OAuth配置
GOOGLE_CLIENT_ID=your_google_oauth_client_id_here
GOOGLE_CLIENT_SECRET=your_google_oauth_client_secret_here

# AI API配置
KIEAI_APIKEY=your_kie_ai_api_key_here

# 支付系统配置
CREEM_KEY=your_creem_production_key_here
CREEM_TEST_KEY=your_creem_test_key_here
CREEM_WEBHOOK_SECRET=your_webhook_secret_here

# Analytics配置
GOOGLE_ANALYTICS_ID=G-XXXXXXX
GOOGLE_ADS_ID=pub-xxxxxxxxxxxxxxxx

# 数据库和存储ID（从Cloudflare控制台获取）
D1_DATABASE_ID=your_d1_database_id_here
KV_NAMESPACE_ID=your_kv_namespace_id_here
R2_BUCKET_NAME=your_r2_bucket_name_here